{
  "name": "web-component-lib",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "vite build && tsc -b",
    "test": "bun test",
    "test:update-snapshot": "bun test --update-snapshots",
    "lint": "bun run lint:check",
    "lint:check": "biome lint",
    "lint:fix": "biome format --write && biome lint --apply",
    "format:check": "biome format",
    "format:fix": "biome format --write",
    "preinstall": "npx only-allow bun"
  },
  "keywords": [],
  "author": {
    "name": "Yuta (https://github.com/yutalinux)"
  },
  "license": "MIT",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/yutalinux/web-component-lib.git"
  },
  "packageManager": "bun@1.1.30",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "react": "^18.3.1",
    "react-aria-components": "^1.4.0",
    "react-dom": "^18.3.1",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@chromatic-com/storybook": "^2.0.2",
    "@happy-dom/global-registrator": "^15.7.4",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-themes": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@storybook/theming": "^8.3.5",
    "@types/bun": "latest",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.4"
  }
}
